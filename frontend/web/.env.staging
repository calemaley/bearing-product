# Staging Environment
VITE_API_BASE_URL=https://staging-api.yourdomain.com/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_API_URL=https://staging-api.yourdomain.com/auth
VITE_AUTH_TOKEN_KEY=staging_auth_token

# Services
VITE_ANALYTICS_ID=UA-XXXXXXXXX-X
VITE_SENTRY_DSN=https://key@sentry.io/staging-project-id
VITE_STRIPE_PUBLIC_KEY=pk_test_XXXXXXXXX

# Debug Settings
VITE_DEBUG=true
VITE_LOG_LEVEL=warn
VITE_REACT_STRICT_MODE=true

# Features
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_DARK_MODE=true

# App Info
VITE_APP_NAME=Bearing Specification (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_APP_ENV=staging
